    "@tsconfig/svelte": "^3.0.0",
    "cross-env": "^7.0.3",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0-beta.7"
  }
}
